{
   "expo": {
      "name": "SwiftDesk",
      "slug": "SwiftDesk",
      "version": "1.0.0",
      "orientation": "portrait",
      "icon": "./assets/icon.png",
      "userInterfaceStyle": "light",
      "splash": {
         "image": "./assets/splash.png",
         "resizeMode": "contain",
         "backgroundColor": "#EAEFF5"
      },
      "assetBundlePatterns": ["**/*"],
      "ios": {
         "supportsTablet": true,
         "bundleIdentifier": "com.isimisi.test",
         "infoPlist": {
            "NFCReaderUsageDescription": "This app uses NFC to register office seat entry."
         }
      },
      "android": {
         "adaptiveIcon": {
            "foregroundImage": "./assets/adaptive-icon.png",
            "backgroundColor": "#EAEFF5"
         },
         "permissions": ["android.permission.NFC"],
         "package": "com.isimisi.test",
         "config": {
            "googleMaps": {
               "apiKey": "AIzaSyDhA5PsmYlPSIkldhmuGrfPbQApb8heWgE"
            }
         }
      },
      "web": {
         "favicon": "./assets/favicon.png"
      },
      "plugins": ["react-native-nfc-manager"],
      "extra": {
         "eas": {
            "projectId": "ce6b1ac5-660c-4a48-b50e-67798e57f903"
         }
      }
   }
}
